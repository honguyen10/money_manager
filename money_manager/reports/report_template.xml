<odoo>
    <data>
        <template id="income_report_document">
            <t t-call="web.external_layout">
            <div class="page">
                <p>hi</p>
            </div>
                
            </t>
        </template>
        <template id="income_report">
            <t t-call="web.html_container">
                <t t-foreach="docs" t-as="o">
                    <t t-call="money_manager.income_report_document" t-lang="user.partner_id.lang"/>
                </t>
            </t>
        </template>
        <report
            id="action_print_income_report"
            string="Income Report"
            model="report.wizard"
            report_type="qweb-pdf"
            file="money_manager.income_report" 
            name="money_manager.income_report"
            print_report_name="'%s' %(object.partner_id.name)"
        />
    </data>
</odoo>
