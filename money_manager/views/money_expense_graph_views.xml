<odoo>
    <record id="money_expense_graph_view" model="ir.ui.view">
        <field name="name">money.expense.graph.view</field>
        <field name="model">money.expense</field>
        <field name="arch" type="xml">
            <graph string="Expense">
                <field name="category"/>
                <field name="amount"/>
            </graph>
        </field>
    </record>

    <record model="ir.actions.act_window" id="money_expense_report_action">
        <field name="name">Expense Report</field>
        <field name="res_model">money.expense</field>
        <field name="view_mode">graph</field>
    </record>

</odoo>
